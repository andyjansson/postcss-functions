{
  "name": "postcss-functions",
  "version": "3.0.0",
  "description": "PostCSS plugin for exposing JavaScript functions",
  "main": "dist/index.js",
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "prepublish": "npm run build && del-cli dist/__tests__",
    "build": "del-cli dist && cross-env BABEL_ENV=publish babel src --out-dir dist",
    "pretest": "eslint src && npm run build",
    "test": "nyc ava src/__tests__/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andyjansson/postcss-functions.git"
  },
  "keywords": [
    "postcss",
    "postcss-plugin",
    "javascript",
    "function",
    "functions"
  ],
  "author": "Andy Jansson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andyjansson/postcss-functions/issues"
  },
  "homepage": "https://github.com/andyjansson/postcss-functions",
  "dependencies": {
    "glob": "^7.1.2",
    "object-assign": "^4.1.1",
    "postcss-value-parser": "^4.0.1"
  },
  "peerDependencies": {
    "postcss": "^8.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.5.5",
    "ava": "^2.2.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-istanbul": "^5.2.0",
    "cross-env": "^5.0.5",
    "del-cli": "^2.0.0",
    "eslint": "^6.1.0",
    "eslint-config-i-am-meticulous": "^11.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.7.0",
    "nyc": "^14.1.1",
    "postcss": "^8.1.4"
  },
  "ava": {
    "compileEnhancements": false,
    "require": "@babel/register"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint-config-i-am-meticulous"
  }
}
